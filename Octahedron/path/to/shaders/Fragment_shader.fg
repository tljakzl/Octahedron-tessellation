#version 330 core

//in vec4 vertexColor;
in vec3 ourColor;
in vec2 TexCoord;

out vec4 color;

uniform sampler2D ourTexture1;
uniform sampler2D ourTexture2;
uniform float visibility_level = 1.0f;


void main()
{
  
  //color=vertexColor;
  //color=ourColor;
  color = mix(texture(ourTexture1, TexCoord),texture(ourTexture2, TexCoord),visibility_level);

}